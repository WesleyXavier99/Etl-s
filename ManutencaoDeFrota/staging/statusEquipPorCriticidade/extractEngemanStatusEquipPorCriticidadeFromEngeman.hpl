  <transform>
    <name>LoadEngemanStatusEquipPorCriticidade</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>DBO_ORACLE_ENGEMAN</connection>
    <execute_each_row>N</execute_each_row>
    <limit>0</limit>
    <sql>SELECT
      AP.CODAPL,
      FL.CODFIL,
      FL.RAZSOC AS NAVIO, 
      AP.TAG AS TAGEQUIP,
      AP.DESCRICAO AS EQUIPAMENTO,
      U_PK_CODIGO AS CODCRITICIDADE,
      UC.U_DESC AS CRITICIDADE,
      AP.MODELO,
      AP.NUMSER AS NUMERO_SERIE,
      AP.NUMPAT AS PART_NUMBER,
      FN.CODFOR,
      FN.RAZSOC AS FABRICANTE,
      LA.CODLOCAPL,
      LA.DESCRICAO AS LOCALIZACAO,
      TA.CODTIPAPL,
      TA.DESCRICAO AS TIPO      
       
FROM ENGEMANTVVLOGIN.APLIC AP
LEFT JOIN ENGEMANTVVLOGIN.FILIAL FL ON FL.CODFIL=AP.CODFIL
LEFT JOIN ENGEMANTVVLOGIN.USER_CLACOM UC ON AP.R_USER_CLACOM_U_PK_CODIGO=UC.U_PK_CODIGO
LEFT JOIN ENGEMANTVVLOGIN.FORNECEDOR FN ON AP.CODFOR=FN.CODFOR 
LEFT JOIN ENGEMANTVVLOGIN.LOCAPLIC LA ON LA.CODLOCAPL=AP.CODLOCAPL
LEFT JOIN ENGEMANTVVLOGIN.TIPAPLIC TA ON AP.CODTIPAPL=TA.CODTIPAPL

WHERE AP.CODEMP=1
AND FL.CODFIL IN (8,58,61,72,74,75)

ORDER BY FL.RAZSOC, AP.TAGGRU, AP.TAG</sql>
