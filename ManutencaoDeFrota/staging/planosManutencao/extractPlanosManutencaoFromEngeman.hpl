    <execute_each_row>N</execute_each_row>
    <limit>0</limit>
    <sql>SELECT
	CAST(PM.CODPLA AS INT) AS CODPLA
	,CAST(PM.CODEMP AS INT) AS CODEMP
	,PM.DESCRICAO
	,PM.TAG
	,(CASE 
        WHEN SUBSTR(PM.TAG,1,4) IN  ('CERT','MSAI','EXER') THEN 'CLASSE'
        ELSE SUBSTR(PM.TAG,1,4)
      END) AS GRUPO
     ,PM.ATIVO
     ,PM.DATALT 
FROM
	ENGEMANTVVLOGIN.PLAMANUT PM</sql>
