    <execute_each_row>N</execute_each_row>
    <limit>0</limit>
    <sql>--DIMENS√ÉO EQUIPAMENTO
SELECT
	CAST(AP.CODAPL AS INT) AS CODAPL,
	AP.DESCRICAO,
	QG.DESCRICAO AS GRUPO_EQUIPAMENTO,
	AP.TAG,
	AP.MODELO,
	AP.NUMSER,
	AP.NUMPAT
FROM
	ENGEMANTVVLOGIN.APLIC AP
LEFT JOIN (SELECT CODAPL, SUBSTR(TAGGRU,1,7) AS CODGRUP FROM ENGEMANTVVLOGIN.APLIC) QAXG 
ON QAXG.CODAPL=AP.CODAPL
LEFT JOIN (SELECT TAG,DESCRICAO,TAGGRU AS CODGRUP FROM ENGEMANTVVLOGIN.APLIC WHERE LENGTH(TAGGRU)=7 AND DESCRICAO LIKE '%SISTEMA%') QG 
ON QAXG.CODGRUP=QG.CODGRUP
</sql>
