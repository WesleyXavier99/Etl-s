    <connection>DBO_ORACLE_ENGEMAN</connection>
    <execute_each_row>N</execute_each_row>
    <limit>0</limit>
    <sql>SELECT
      CAST(APLIC.CODAPL AS INT)    	AS  CODAPL, 
      APLIC.TAG         			AS  TAGEQUIP,
      APLIC.DESCRICAO   			AS  EQUIPAMENTO,
      CAST(FILIAL.CODFIL AS INT)   	AS  CODFIL,
      FILIAL.RAZSOC     			AS  NAVIO,
      CAST(COLACU.VALOR AS INT)		AS  HORIMETRO,
      COLACU.DATHOR				AS  DATA_HORIMETRO
FROM ENGEMANTVVLOGIN.COLACU
LEFT JOIN ENGEMANTVVLOGIN.APLIC   ON    APLIC.CODAPL  = COLACU.CODAPL
LEFT JOIN ENGEMANTVVLOGIN.FILIAL  ON    FILIAL.CODFIL = APLIC.CODFIL

WHERE APLIC.CODEMP  = 1
AND   APLIC.ATIVO   = 'S'
AND   APLIC.CODFIL  > 7

ORDER BY FILIAL.CODFIL, APLIC.CODAPL, COLACU.DATHOR, COLACU.VALOR</sql>
