  <transform>
    <name>LoadDimTiposPlanoEngeman</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>DBO_ORACLE_ENGEMAN</connection>
    <execute_each_row>N</execute_each_row>
    <limit>0</limit>
    <sql>SELECT
	DISTINCT TIPO,
	CASE 
		WHEN TIPO = 'DATA_ESPECIFICA' 	THEN '1'
		WHEN TIPO = 'PERIODICOS' 		THEN '2'
		WHEN TIPO = 'ACUMULATIVO'		THEN '3'
	END AS CODTIPO
FROM
	(
	SELECT
		DISTINCT CODPLA,
		CODAPL,
		'DATA_ESPECIFICA' AS TIPO
	FROM
		ENGEMANTVVLOGIN.PLADAT
	WHERE
		CODEMP = 1
		AND ATIVO = 'S'
UNION ALL
	SELECT
		DISTINCT CODPLA,
		CODAPL,
		'ACUMULATIVO'
	FROM
		ENGEMANTVVLOGIN.PLAACU
	WHERE
		CODEMP = 1
		AND ATIVO = 'S'
UNION ALL
	SELECT
		DISTINCT CODPLA,
		CODAPL,
		'PERIODICOS'
	FROM
		ENGEMANTVVLOGIN.PLAPER
	WHERE
		CODEMP = 1
		AND ATIVO = 'S')</sql>
