    <connection>DBO_ORACLE_SUPRAVIZIO</connection>
    <sql>SELECT
	DISTINCT 
	O.ID_OCORRENCIA,
	O.NUMERO,
	O.ASSUNTO,
	O.DATA_HORA_CRIACAO DATA_HORA_ABERTURA,
	O.DATA_HORA_FIM_REAL DATA_FECHAMENTO,
	CPO.PDA_ACA,
	CPO.PDA_ATRACACAO,
	CPO.PDA_DESATRACACAO,
	O.SITUACAO,
	CPO.PDA_NAVIO,
	CPO.PDA_ORGANIZACAO,
	CPO.PDA_PORTO,
	CPO.PDA_VIAGEM,
	CPO.PDA_DESCRICAO,
	CPO.PDA_LIBERACAO,
	CPO.PDA_FATURAR,
	CPO.PDA_VALORPO,
	CPO.OBS_FROTA,
	CPO.OBS_GRUPO_FROTA,
	CPO.PDA_SOLICITANTE,
	PDA.PDA_VALOR,
	PDA.TECNICO_TRIPULANTE,
	PDA.PDA_SOLICITANTE AS ENCAMINHAMENTO,
	PDA.DATA_SERVICO,
	RESP.NOME AS RESPONSAVEL,
	CLI.NOME AS CLIENTE,
	CLI.TELEFONE,
	CLI.EMAIL,
	EMPCLIORG.DESCRICAO EMPRESA,
	CLIORG.DESCRICAO AREA,
	CSP.DESCRICAO CLASSE_SUB_PROC,
	DP.VERSAO,
	CS.DESCRICAO CLASSE_SERVICO,
	SRV.DESCRICAO SERVICO,
	AT.DESCRICAO AS SITUACAO_ATV,
	CPO.PDA_FORNECEDOR
FROM
	SV.OCORRENCIA O
	LEFT OUTER JOIN SV.CP_ORDEM_SERVICO CPO ON
		O.ID_OCORRENCIA = CPO.ID_OCORRENCIA
	LEFT OUTER JOIN SV.Z_00069_PDA_GRID_SERVICO PDA ON
		O.ID_OCORRENCIA = PDA.ID_OCORRENCIA
	INNER JOIN SV.DESENHO_PROCESSO DP
		ON O.ID_DESENHO_PROCESSO = DP.ID_DESENHO_PROCESSO 
	INNER JOIN SV.CLASSE_SUB_PROCESSO CSP ON
		O.ID_CLASSE_SUB_PROC = CSP.ID_CLASSE_SUB_PROCESSO
	INNER JOIN SV.PESSOA RESP ON
		O.ID_RESPONSAVEL = RESP.ID_PESSOA
	INNER JOIN SV.PESSOA CLI ON
		O.ID_CLIENTE = CLI.ID_PESSOA
	INNER JOIN SV.ORGAO CLIORG
		ON CLI.ID_ORGAO = CLIORG.ID_ORGAO 
	INNER JOIN SV.EMPRESA EMPCLIORG
		ON CLIORG.ID_EMPRESA = EMPCLIORG.ID_EMPRESA 
	INNER JOIN SV.ATIVIDADE AT ON
		O.ID_ATIVIDADE = AT.ID_ATIVIDADE
	LEFT OUTER JOIN SV.SERVICO SRV ON
		PDA.SERVICO = SRV.ID_SERVICO
	LEFT OUTER JOIN SV.CLASSE_SERVICO CS
		ON SRV.ID_CLASSE_SERVICO = CS.ID_CLASSE_SERVICO
WHERE
	TO_CHAR(O.DATA_HORA_CRIACAO, 'YYYY-MM-DD') BETWEEN ? AND ?
	AND CSP.SIGLA = 'PDAF'
	AND O.SITUACAO &lt;&gt; 'Cancelada'
UNION
SELECT
	DISTINCT 
	O.ID_OCORRENCIA,
	O.NUMERO,
	O.ASSUNTO,
	O.DATA_HORA_CRIACAO DATA_HORA_ABERTURA,
	O.DATA_HORA_FIM_REAL DATA_FECHAMENTO,
	CPO.PDA_ACA,
	CPO.PDA_ATRACACAO,
	CPO.PDA_DESATRACACAO,
	O.SITUACAO,
	CPO.PDA_NAVIO,
	CPO.PDA_ORGANIZACAO,
	CPO.PDA_PORTO,
	CPO.PDA_VIAGEM,
	CPO.PDA_DESCRICAO,
	CPO.PDA_LIBERACAO,
	CPO.PDA_FATURAR,
	CPO.PDA_VALORPO,
	CPO.OBS_FROTA,
	CPO.OBS_GRUPO_FROTA,
	CPO.PDA_SOLICITANTE,
	PDA.PDA_VALOR,
	PDA.TECNICO_TRIPULANTE,
	PDA.PDA_SOLICITANTE AS ENCAMINHAMENTO,
	PDA.DATA_SERVICO,
	RESP.NOME AS RESPONSAVEL,
	CLI.NOME AS CLIENTE,
	CLI.TELEFONE,
	CLI.EMAIL,
	EMPCLIORG.DESCRICAO EMPRESA,
	CLIORG.DESCRICAO AREA,
	CSP.DESCRICAO CLASSE_SUB_PROC,
	DP.VERSAO,
	CS.DESCRICAO CLASSE_SERVICO,
	SRV.DESCRICAO SERVICO,
	AT.DESCRICAO AS SITUACAO_ATV,
	CPO.PDA_FORNECEDOR
FROM
	SV.OCORRENCIA O
	LEFT OUTER JOIN SV.CP_ORDEM_SERVICO CPO ON
		O.ID_OCORRENCIA = CPO.ID_OCORRENCIA
	LEFT OUTER JOIN SV.Z_00069_PDA_GRID_SERVICO PDA ON
		O.ID_OCORRENCIA = PDA.ID_OCORRENCIA
	INNER JOIN SV.DESENHO_PROCESSO DP
		ON O.ID_DESENHO_PROCESSO = DP.ID_DESENHO_PROCESSO 
	INNER JOIN SV.CLASSE_SUB_PROCESSO CSP ON
		O.ID_CLASSE_SUB_PROC = CSP.ID_CLASSE_SUB_PROCESSO
	INNER JOIN SV.PESSOA RESP ON
		O.ID_RESPONSAVEL = RESP.ID_PESSOA
	INNER JOIN SV.PESSOA CLI ON
		O.ID_CLIENTE = CLI.ID_PESSOA
	INNER JOIN SV.ORGAO CLIORG
		ON CLI.ID_ORGAO = CLIORG.ID_ORGAO 
	INNER JOIN SV.EMPRESA EMPCLIORG
		ON CLIORG.ID_EMPRESA = EMPCLIORG.ID_EMPRESA 
	INNER JOIN SV.ATIVIDADE AT ON
		O.ID_ATIVIDADE = AT.ID_ATIVIDADE
	LEFT OUTER JOIN SV.SERVICO SRV ON
		PDA.SERVICO = SRV.ID_SERVICO
	LEFT OUTER JOIN SV.CLASSE_SERVICO CS
		ON SRV.ID_CLASSE_SERVICO = CS.ID_CLASSE_SERVICO
WHERE
	TO_CHAR(O.DATA_HORA_CRIACAO, 'YYYY-MM-DD') BETWEEN ? AND ?
	AND CSP.SIGLA = 'SP'
	AND CPO.PFP_TIPO = 'Agenciamento da Frota'
	AND O.SITUACAO &lt;&gt; 'Cancelada'
ORDER BY
	3 DESC</sql>
